{
  "name": "hot-view-graphql-server",
  "private": true,
  "description": "hot view graphql server",
  "version": "0.0.1",
  "author": "Apollo <packages@apollographql.com>",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dave-lab12/hot-view-graphql-server"
  },
  "homepage": "https://github.com/Dave-lab12/hot-view-graphql-server#readme",
  "bugs": {
    "url": "https://github.com/Dave-lab12/hot-view-graphql-server/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18.0"
  },
  "scripts": {
    "build": "tsc --build ",
    "clean": "git clean -dfqX",
    "prepack": "npm run build",
    "prettier-check": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "publish-changeset": "changeset publish",
    "spell-check": "cspell lint '**' --no-progress || (echo 'Add any real words to cspell-dict.txt.'; exit 1)",
    "test": "jest",
    "test:ci": "jest --coverage --ci --maxWorkers=2 --reporters=default --reporters=jest-junit",
    "watch": "tsc --build --watch",
    "dev": "nodemon src/index.ts",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "2.26.0",
    "@graphql-codegen/cli": "2.16.2",
    "@graphql-codegen/typescript": "2.8.6",
    "@graphql-codegen/typescript-resolvers": "2.7.11",
    "@types/jest": "29.2.5",
    "@types/node": "14.18.36",
    "cspell": "6.18.1",
    "jest": "29.3.1",
    "jest-junit": "15.0.0",
    "nodemon": "2.0.20",
    "prettier": "2.8.1",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "volta": {
    "node": "18.12.1",
    "npm": "9.2.0"
  },
  "dependencies": {
    "@apollo/datasource-rest": "^4.3.2",
    "@apollo/server": "^4.3.0",
    "apollo-datasource-rest": "^3.7.0",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "node-fetch": "^3.3.0"
  }
}
