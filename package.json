{
  "name": "hot-view-graphql-server",
  "private": true,
  "description": "hot view graphql server",
  "version": "0.0.1",
  "author": "Apollo <packages@apollographql.com>",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dave-lab12/hot-view-graphql-server"
  },
  "homepage": "https://github.com/Dave-lab12/hot-view-graphql-server#readme",
  "bugs": {
    "url": "https://github.com/Dave-lab12/hot-view-graphql-server/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18.0"
  },
  "scripts": {
    "build": "tsc --build ",
    "clean": "git clean -dfqX",
    "prepack": "npm run build",
    "prettier-check": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "publish-changeset": "changeset publish",
    "spell-check": "cspell lint '**' --no-progress || (echo 'Add any real words to cspell-dict.txt.'; exit 1)",
    "test": "jest",
    "test:ci": "jest --coverage --ci --maxWorkers=2 --reporters=default --reporters=jest-junit",
    "watch": "tsc --build --watch",
    "dev": "nodemon src/index.ts",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "2.26.1",
    "@graphql-codegen/cli": "2.16.5",
    "@graphql-codegen/typescript": "2.8.8",
    "@graphql-codegen/typescript-resolvers": "2.7.13",
    "@types/jest": "29.5.2",
    "@types/node": "14.18.51",
    "cspell": "6.31.1",
    "jest": "29.5.0",
    "jest-junit": "16.0.0",
    "nodemon": "2.0.22",
    "prettier": "2.8.8",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.3"
  },
  "volta": {
    "node": "18.16.1",
    "npm": "9.7.2"
  },
  "dependencies": {
    "@apollo/datasource-rest": "^4.3.2",
    "@apollo/server": "^4.3.0",
    "apollo-datasource-rest": "^3.7.0",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "node-fetch": "^3.3.0"
  }
}
